<div class="flex-item">
  {{#each hbsObject.pageData.section}}
    <h2 id="{{this.id}}" class="anchor-link">{{this.title}} <span>#</span></h2>
    <p>{{this.description}}</p>
      {{#each code}}
        <div class="styleguide-code">
          <div class="msg {{this.alert-class}}" role="alert" aria-live="assertive">
            <div class="msg__icon" aria-label="{{this.aria-label}}"></div>
            <div class="msg__body">
              {{#if this.close}}
                <div class="msg__close">
                  <button class="close-x" type="button" aria-label="Close"></button>
                </div>
              {{/if}}
              <p class="msg__title">{{this.title}}</p>
              <p class="msg__message">{{this.message}}</p>
            </div>
          </div>
        </div>
{{!-- &lt;div&gt;yee&lt;/div&gt; --}}

{{!-- {{escapeHtml snippet}} --}}
        {{!-- {{this.snippet}} --}}
<button class="btn" id="test" data-clipboard-text="Just because you can doesn't mean you should â€” clipboard.js">
    Copy to clipboard
</button>
        <pre class="line-numbers test" data-clipboard-text="this"><code class="language-markup"></code></pre>
        {{!-- <pre class="line-numbers"><code class="language-markup">&lt;p class="2020" id="ok2o"&gt;one&lt;/p&gt;</code></pre> --}}
        {{!-- <div class="view-code">{{this.snippet}}</div> --}}
      {{/each}}
  {{/each}}
</div>
